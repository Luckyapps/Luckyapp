{
  "name": "Luckyblocker",
  "description": "Der Adblocker",
  "version": "1.23041.0",
  "manifest_version": 3,
  "permissions": [
    "storage"
  ],

  "icons": { 
    "128": "logo_128.png"
  },

  "action": {
    "default_title": "EIN TITEL",
    "default_popup":"options.html"
  },

  "options_ui": {
    "page":"options.html",
    "open_in_tab": false
  },

 "content_scripts": [{
    "js": ["script.js"],
    "matches": ["<all_urls>"]
  }],

  "web_accessible_resources": [
    {
      "resources": [ "*.css"],
      "matches": ["https://news.google.com/*","https://www.google.de/*","https://www.google.com/*","https://de.wikipedia.org/*","https://www.wikipedia.de/*","https://minecraft.fandom.com/*"]
    }]
}